{
  "name": "@semanticjs/napkin-ide",
  "version": "0.0.0",
  "description": "Low/No Code tooling for the automation of things.",
  "main": "./packages/cjs/common.js",
  "scripts": {
    "test": "jest",
    "build": "concurrently \"npm run build:es\" \"npm run build:cjs\"",
    "build:es": "tsc -p tsconfig.es.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "rimraf packages && npm run build && npm publish ./packages --access public",
    "test:coverage": "jest --coverage",
    "watch": "npm run watch:es",
    "watch:es": "tsc -p tsconfig.es.json -w",
    "watch:cjs": "tsc -p tsconfig.cjs.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semanticjs/napkin-ide.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/semanticjs/napkin-ide/issues"
  },
  "homepage": "https://github.com/semanticjs/napkin-ide#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@semanticjs/common": "^0.0.15"
  },
  "module": "./packages/es/common.js",
  "files": [
    "packages/"
  ]
}